{
  "manifest_version": 3,
  "name": "Key Metrics",
  "version": "1.0",
  "description": "Key user and revenue metrics for your product.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
